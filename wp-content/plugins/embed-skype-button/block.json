{
	"apiVersion": "1",
	"name": "sbb/skype-button",
	"title": "Skype Button",
	"description": "Interactive skype button fo call/chat in webpage.",
	"category": "widgets",
	"keywords": [
		"skype button",
		"skype chat button",
		"skype call button"
	],
	"textdomain": "skype-button",
	"attributes": {
		"align": {
			"type": "string",
			"default": ""
		},
		"cId": {
			"type": "string",
			"default": ""
		},
		"skypeID": {
			"type": "string",
			"default": "echo123"
		},
		"type": {
			"type": "string",
			"default": "chat"
		},
		"isWhite": {
			"type": "boolean",
			"default": false
		},
		"size": {
			"type": "number",
			"default": 24
		},
		"alignment": {
			"type": "string",
			"default": "left"
		},
		"padding": {
			"type": "object",
			"default": {
				"vertical": "0px",
				"horizontal": "0px"
			}
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"html": false
	},
	"editorScript": "file:dist/editor.js"
}